{
  "python.linting.pylintEnabled": false,
  "python.linting.enabled": true,
  "python.linting.pep8Enabled": true,
  "languageserver": {
      "haskell": {
        "command": "hie-wrapper",
        "rootPatterns": [
          ".stack.yaml",
          "cabal.config",
          "package.yaml"
        ],
        "filetypes": [
          "hs",
          "lhs",
          "haskell"
        ],
        "initializationOptions": {
          "languageServerHaskell": {
          }
        }
      }
    },
    "suggest.autoTrigger": "always"
}
